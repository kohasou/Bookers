<h1>編集画面</h1>

<%= form_for(@book, url: book_path(@book)) do |f| %>

    <div>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </div>
    <div>
        <%= f.label :body %><br>
        <%= f.text_area :body %>
    </div>
    <div><%= f.submit "Updata Book" %></div>
<% end %>